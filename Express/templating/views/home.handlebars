<h1>Bio information of users</h1>
<table id="example" class="table table-striped" style="width:100%">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Username</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    <tr>
        {{#each  data}}
          <th scope="row">{{this.name}}</th>
            <td>{{this.username}}</td>
            <td>{{this.email}}</td>
          </tr>
        {{/each }}
    </tbody>
</table>